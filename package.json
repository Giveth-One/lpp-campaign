{
  "name": "lpp-campaign",
  "version": "0.0.14",
  "description": "Simple campaign plugin with reviewer for liquidpledging.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "[ ! -d build ] && npm run sol-compile; mocha --harmony",
    "sol-compile": "solcpiler --solc-version v0.4.18+commit.9cf6e910",
    "js-compile": "babel --source-maps -d lib/ js/",
    "lint": "solium --dir contracts",
    "compile": "npm run sol-compile; npm run js-compile",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giveth/lpp-campaign.git"
  },
  "keywords": [
    "liquid",
    "pledging",
    "ethereum",
    "solidity",
    "campaign"
  ],
  "author": "perissology",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Giveth/lpp-campaign/issues"
  },
  "homepage": "https://github.com/Giveth/lpp-campaign#readme",
  "dependencies": {
    "@aragon/os": "3.0.4",
    "chai": "^4.1.2",
    "giveth-common-contracts": "0.4.0",
    "liquidpledging": "0.0.10",
    "minimetoken": "0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.3.0",
    "ganache-cli": "^7.0.0-beta.0",
    "lerna": "^2.2.0",
    "solcpiler": "https://github.com/perissology/solcpiler.git#b60ac51b",
    "solium": "^1.0.9",
    "web3": "^1.0.0-beta.24"
  }
}
